{
  "schemaVersion": 1,
  "id": "structure-helpers",
  "version": "${version}",
  "name": "Structure Helpers",
  "description": "Adds miscellaneous structure processors for mod authors to use.",
  "authors": [
    "kwerti",
    "Lilliana \"RoboSky\""
  ],
  "license": "MIT",
  "icon": "assets/structure-helpers/icon.png",
  "environment": "*",
  "depends": {
    "minecraft": ">=1.17-rc.1",
    "fabricloader": ">=0.10.0",
    "fabric-networking-api-v1": "1.0.x",
    "fabric-object-builder-api-v1": "1.10.x"
  },
  "recommends": {
    "fabric-registry-sync-v0": "*"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "modmenu:api": true
  },
  "entrypoints": {
    "main": [
      "robosky.structurehelpers.StructureHelpers"
    ],
    "client": [
      "robosky.structurehelpers.StructureHelpersClient"
    ]
  },
  "mixins": [
    "structhelp.mixins.json"
  ],
  "accessWidener": "structhelp.aw"
}
